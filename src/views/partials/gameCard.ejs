<div class="game-card">

  <!-- <h2>bonjoure je suis <%= jeu.nom %> </h2> -->
  <div class="card-image">
    <img src="/images/<%= jeu.thumbnail %>" alt="">
  </div>
  <div class="card-title">
    <h2><%= jeu.nom %></h2>
  </div>
  <div class="card-desc">
    <p><%= jeu.desc %></p>
  </div>
  <div class="card-cycle">
    <h3>Cycle <span class="cycle cycle-<%= jeu.cycle %>"><%= jeu.cycle %> </span></h3>
  </div>
  <div class="card-tags">
    <% jeu.themes.forEach(element => { %>
      <span class="card-tag"><%= element %> </span>
    <% }) %>
  </div>

</div>